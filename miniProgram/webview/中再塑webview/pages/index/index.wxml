<!--pages/index/index.wxml-->
<view class="wx_dialog_container" hidden="{{firstIn}}">
  <view class="wx-mask"></view>
  <view class="wx-dialog">
    <view class="wx-dialog-title">提示</view>
    <view class="wx-dialog-content">是否要退出小程序</view>
    <view class="wx-dialog-footer">
      <navigator class="wx-dialog-btn" open-type="exit" target="miniProgram">退出</navigator>
      <view class="wx-dialog-btn" catchtap="bindConfirm">取消</view>
    </view>
  </view>
</view>